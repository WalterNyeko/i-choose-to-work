{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-cors": "^0.11.3",
        "bodunde/geocoder": "^1.2",
        "cviebrock/eloquent-sluggable": "^4.6",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "~6.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/scout": "^7.1",
        "laravel/tinker": "^1.0",
        "spatie/laravel-permission": "^2.29",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "teamtnt/laravel-scout-tntsearch-driver": "^7.0",
        "toin0u/geotools-laravel": "^1.0",
        "trexology/reviewrateable": "^2.0",
        "tucker-eric/eloquentfilter": "^1.4"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": ["app/Helpers/permissions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
